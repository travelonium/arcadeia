{
  "Session": {
    "IdleTimeoutSeconds": 600
  },
  "Thumbnails": {
    "Database": {
      "Name": "Thumbnails.sqlite",
      "Path": "data"
    },
    "Video": {
      "Large": {
        "Width": 480
      },
      "Medium": {
        "Width": 320
      },
      "Small": {
        "Width": 160
      },
      "Sprite": {
        "Count": 60,
        "Width": 160,
        "Sprite": 1,
        "Crop": 0
      },
      "T": {
        "Count": 24,
        "Width": 480,
        "Height": 272,
        "Crop": 0
      }
    },
    "Photo": {
      "Large": {
        "Width": 480
      },
      "Medium": {
        "Width": 320
      },
      "Small": {
        "Width": 160
      },
      "T": {
        "Count": 1,
        "Width": 480,
        "Height": 272,
        "Crop": 0
      }
    }
  },
  "Streaming": {
    "Segments": {
      "Duration": 10
    }
  },
  "SupportedExtensions": {
    "Audio": [],
    "Video": [
      ".mp4",
      ".wmv",
      ".flv",
      ".mov",
      ".avi",
      ".asf",
      ".vob",
      ".mkv",
      ".webm"
    ],
    "Photo": [
      ".cr2",
      ".gif",
      ".bmp",
      ".png",
      ".jpg",
      ".jpeg",
      ".tif",
      ".tiff",
      ".heic",
      ".webp"
    ]
  },
  "FFmpeg": {
    "Path": "/usr/bin",
    "TimeoutMilliseconds": 30000
  },
  "yt-dlp": {
    "Path": "/usr/local/bin",
    "Options": []
  },
  "Solr": {
    "URL": "http://solr:8983/solr/Library"
  },
  "Scanner": {
    "StartupScan": true,
    "StartupUpdate": true,
    "StartupCleanup": false,
    "ForceGenerateMissingThumbnails": false,
    "PeriodicScanIntervalMilliseconds": 3600000,
    "ParallelScannerTasks": 4,
    "WatchedFolders": [],
    "Folders": [
      "/Network",
      "/Uploads"
    ],
    "IgnoredPatterns": [
      "\\/\\..*",
      "^\\..*$"
    ]
  },
  "Mounts": [],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "KnownProxies": []
}