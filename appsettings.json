{
  "MediaLibrary": {
    "Name": "Media.xml",
    "Path": ".data"
  },
  "ThumbnailsDatabase": {
    "Name": "Thumbnails.sqlite",
    "Path": ".data",
    "Maximum": 24
  },
  "SupportedExtensions": {
    "Audio": [],
    "Video": [
      ".mp4",
      ".wmv",
      ".flv",
      ".mov",
      ".avi"
    ],
    "Photo": []
  },
  "FFmpeg": {
    "Path": "/usr/local/bin",
    "Timeout": 5000
  },
  "Scanner": {
    "StartupScan": false,
    "StartupUpdate": false,
    "StartupCleanup": false,
    "WatchedFolders": [
      "/Volumes"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "KnownProxies": []
}
