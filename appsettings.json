{
  "MediaLibrary": {
    "Name": "Library.xml",
    "Path": "data"
  },
  "ThumbnailsDatabase": {
    "Name": "Thumbnails.sqlite",
    "Path": "data",
    "Maximum": 24
  },
  "SupportedExtensions": {
    "Audio": [],
    "Video": [
      ".mp4",
      ".wmv",
      ".flv",
      ".mov",
      ".avi"
    ],
    "Photo": []
  },
  "FFmpeg": {
    "Path": "/usr/bin",
    "Timeout": 5000
  },
  "Scanner": {
    "StartupScan": false,
    "PeriodicScan": false,
    "StartupUpdate": false,
    "StartupCleanup": false,
    "WatchedFolders": [
      "/Uploads",
      "/Network"
    ],
    "Period": 3600000
  },
  "Solr": {
    "URL": "http://solr:8983/solr/Library"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "KnownProxies": []
}
