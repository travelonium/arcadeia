{
  "MediaLibrary": {
    "Name": "Library.xml",
    "Path": "data"
  },
  "Thumbnails": {
    "Database": {
      "Name": "Thumbnails.sqlite",
      "Path": "data",
      "Maximum": 24
    }
  },
  "SupportedExtensions": {
    "Audio": [],
    "Video": [
      ".mp4",
      ".wmv",
      ".flv",
      ".mov",
      ".avi",
      ".asf",
      ".vob"
    ],
    "Photo": [
      ".cr2",
      ".gif",
      ".bmp",
      ".png",
      ".jpg",
      ".jpeg",
      ".tif",
      ".tiff",
      ".heic",
      ".webp"
    ]
  },
  "FFmpeg": {
    "Path": "/usr/bin",
    "Timeout": 5000
  },
  "Solr": {
    "URL": "http://solr:8983/solr/Library"
  },
  "Scanner": {
    "StartupScan": false,
    "PeriodicScan": false,
    "StartupUpdate": false,
    "StartupCleanup": false,
    "WatchedFolders": [
      "/Uploads",
      "/Network"
    ],
    "IgnoredFileNames": [
      "^\\..*$"
    ],
    "Period": 3600000
  },
  "Mounts": [],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "KnownProxies": []
}
