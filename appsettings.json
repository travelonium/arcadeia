    "Folders": [
      "/Network"
    ],
    "IgnoredFileNames": [
      "\\/\\..*",
      "^\\..*$"
    ]
  },
  "Mounts": [],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
