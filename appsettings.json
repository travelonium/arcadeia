      "ParallelScannerTasks": 4,
      "WatchedFolders": [],
      "Folders": [
         "/Network",
         "/Uploads"
      ],
      "IgnoredPatterns": [
         "\\/\\..*",
         "^\\..*$"
      ]
   },
   "Mounts": [],
   "Logging": {
      "LogLevel": {
         "Default": "Information",
         "Microsoft": "Warning",
         "Microsoft.AspNetCore.SpaProxy": "Information",
         "Microsoft.Hosting.Lifetime": "Information"
      }
